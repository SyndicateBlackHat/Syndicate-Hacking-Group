import discord
from discord.ext import commands
bot = commands.Bot(command_prefix='$'
                   @bot.event
                   async def on.ready():
                   print ('Bot is ready to calculate!')
@bot.command()
async def add(ctx, a: int, b: int):
    await ctx.send(a+b)
@bot.command()
async def add (ctx, a: int, b: int):
    await ctx.send(a-b)
@bot.command()
async def (ctx, a: int, b:i int):
    ctx.send(a*b)
@bot.command()
async def (ctx, a: int, b: int):
    ctx.send(a:b)
